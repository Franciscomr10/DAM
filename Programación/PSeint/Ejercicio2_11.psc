Algoritmo Ejercicio2_11
	Para n<-0 Hasta 10 Con Paso 1 Hacer
		Escribir " ", n, " " Sin Saltar
	FinPara
	Escribir " "
	n<-0
	Mientras n<>11 Hacer
		Escribir " ", n, " " Sin Saltar
		n<-n+1
	Fin Mientras
	Escribir " "
	n<-0
	Repetir
		Escribir " ", n, " " Sin Saltar
		n<-n+1
	Hasta Que n=11
	Escribir " "
FinAlgoritmo
